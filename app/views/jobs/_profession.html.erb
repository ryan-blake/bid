<%= form_for(Job.new, url: new_job_path, method: :get) do |f| %>

  <div class="form-group">
    <%= f.label :Select_Expertise %>
    <%= f.collection_select :profession_id, Profession.all, :id, :title, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= f.submit class: "btn btn-success"%>
  </div>
<% end %>
